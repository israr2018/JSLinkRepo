			$("#gmnotesidp").text("Notes");
			$("#pnrnotesidp").text("Notes");
			$("#cnbnotesidp").text("Notes");
			}
			if(languageis=="ar"){
			$("#gmnotesidp").text("ملاحظات");
			$("#pnrnotesidp").text("ملاحظات");
			$("#cnbnotesidp").text("ملاحظات");
			}
			$("#PNRReview").show();
			$("#aatReview").hide();
			}
			if(RequestStatus=="Waiting for from Authority and Affair Unit"){
