$("#reasontopayid").keyup(function(event){
	var keycode = (event.keyCode ? event.keyCode : event.which);
    if(keycode == '13'){
		//$("#reasontopayid").append(".");
		//$("#reasontopayid").append("\n");
		//$("#reasontopayid").append(".");
		// var txt = 
    var box = $("#reasontopayid");
   // box.val(box.val() + "\n");
        box.val(box.val() + "● ");
	}
	
});
