			$("#verygoodid").val(data.d.results[i].PercentageOfExmployeeVeryGood);
			$("#weakid").val(data.d.results[i].PercentageOfExmployeeWeak);
			$("#goodyid").val(data.d.results[i].PercentageOfEmployeeGoody);
