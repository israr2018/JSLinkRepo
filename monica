			$("#stydyfileid").hide();
			$("#pnrtrid2").show();
			$("#aatrid").hide();
			$("#confirmid").hide();
			$("#GMReview").hide();
			$("#PNRAPPROVEID").hide();
			$("#PNRrejectid").hide();
			$("#CNBAPPROVEID").hide();
			$("#CNGrejectid").hide();
